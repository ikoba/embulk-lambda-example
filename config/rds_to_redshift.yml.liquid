in:
  type: mysql
  host: {{ env.RDS_HOST }}
  user: {{ env.RDS_USER }}
  password: {{ env.RDS_PASSWORD }}
  database: {{ env.RDS_DB }}
  table: {{ env.RDS_TABLE }}
out:
  type: redshift
  host: {{ env.REDSHIFT_HOST }}
  user: {{ env.REDSHIFT_USER }}
  password: {{ env.REDSHIFT_PASSWORD }}
  schema: {{ env.REDSHIFT_SCHEMA }}
  database: {{ env.REDSHIFT_DB }}
  table: {{ env.REDSHIFT_TABLE }}
  s3_bucket: {{ env.REDSHIFT_S3_BUCKET }}
  s3_key_prefix: temp
  aws_auth_method: env
  mode: truncate_insert